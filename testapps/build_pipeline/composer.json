        }
    ],
    "require": {
        "php": "7.1.*",
        "ext-grpc": "*",
        "ext-mailparse": "*",
        "ext-mbstring": "*",
        "ext-opencensus": "*",
        "ext-pdo_sqlite": "*",
        "google/auth": "^0.11",
        "grpc/grpc": "^1.0",
        "silex/silex": "^1.3"
    },
    "scripts": {
        "post-install-cmd": [
            "php gcloud-env.php"
        ]
    },
    "autoload": {
        "classmap": ["web"]
    }
}
