steps:
        - name: gcr.io/cloud-builders/docker
          args: ['build', '-t', '${IMAGE}', '--build-arg', 'BUILDER_TARGET_IMAGE=${BUILDER_TARGET_IMAGE}', '--build-arg', 'BUILDER_TARGET_TAG=${TAG}', '.']
        - name: ${IMAGE}
        - name: gcr.io/cloud-builders/docker
          args: ['pull', '${TEST_RUNNER}']
        - name: gcr.io/cloud-builders/docker
          args: ['run', '-v', '/workspace:/workspace', '${TEST_RUNNER}']
images:
        - '${IMAGE}'
